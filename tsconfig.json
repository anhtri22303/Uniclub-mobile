{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "module": "ESNext",
    "paths": {
      "@*": [
        "./src/*"
      ],
      "@atoms/*": [
        "./src/components/Atoms/*"
      ],
      "@molecules/*": [
        "./src/components/Molecules/*"
      ],
      "@organisms/*": [
        "./src/components/Organisms/*"
      ],
      "@configs/*": [
        "./src/configs/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@stores/*": [
        "./src/stores/*"
      ],
      "@types/*": [
        "./src/types/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@assets/*": [
        "./assets/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}